{
  "cacheDirectory": "{lucee-web}/cache/",
  "cacheDirectoryMaxSize": "100mb",
  "clientDirectory": "{lucee-web}/client-scope/",
  "clientDirectoryMaxSize": "100mb",
  "componentDataMemberAccess": "public",
  "componentUseVariablesScope": true,
  "developMode": false,
  "salt": "C1E1DABC-2CA3-46B6-B3D3B9AC436FFCC3",
  "tempDirectory": "{lucee-web}/temp/",
  "version": "5.0",
  "cfx": {
    "HelloWorld": {
      "type": "java",
      "class": "lucee.cfx.example.HelloWorld"
    }
  },
  "customTagMappings": [
    {
      "physical": "{lucee-web}/customtags/",
      "trusted": "yes"
    }
  ],
  "debugging": {
    "template": "/lucee/templates/debugging/debugging.cfm"
  },
  "fileSystem": {
    "fldDirectory": "{lucee-web}/library/fld/",
    "deployDirectory": "{lucee-web}/cfclasses/",
    "tldDirectory": "{lucee-web}/library/tld/"
  },
  "mappings": {
    "/lucee/": {
      "physical": "{lucee-web}/context/",
      "primary": "physical",
      "trusted": "true",
      "toplevel": "yes",
      "archive": "{lucee-web}/context/lucee-context.lar",
      "readonly": "yes"
    }
  },
  "remoteClients": {
    "directory": "{lucee-web}remote-client/"
  },
  "resourceProviders": [
    {
      "arguments": "case-sensitive:true;lock-timeout:1000;",
      "scheme": "ram",
      "class": "lucee.commons.io.res.type.cache.CacheResourceProvider"
    }
  ]
}